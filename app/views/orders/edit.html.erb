<%=form_for @category do |form| %>
  <% if @category.errors.any? %>
    <ul>
    <% @category.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
    </ul>
  <% end %>
  <div>
    <%= form.label :status %><br>
    <%= form.select :new_status, options_from_collection_for_select(Order.all, :status), :selected =>params[:new_status]%>
  </div>
  <div>
    <%= form.submit %>
  </div>

<% end %>
